                    jcr:title="Page Root"
                    sling:resourceType="wcm/core/components/policy/policy"
                    components="[group:Experience AEM SPA React - Content,/apps/eaem/components/form/container,group:Experience AEM SPA React - Structure]">
                    <jcr:content jcr:primaryType="nt:unstructured"/>
                    <cq:authoring jcr:primaryType="nt:unstructured">
                        <assetToComponentMapping jcr:primaryType="nt:unstructured">
                            <mapping_1575024218483
                                jcr:primaryType="nt:unstructured"
                                assetGroup="media"
                                assetMimetype="image/*"
                                droptarget="image"
                                resourceType="eaem/components/image"/>
                            <mapping_1575030843388
                                jcr:primaryType="nt:unstructured"
                                assetGroup="content"
                                assetMimetype="text/html"
                                droptarget="experiencefragment"
                                resourceType="eaem/components/experiencefragment"/>
                            <mapping_1575030853128
                                jcr:primaryType="nt:unstructured"
                                assetGroup="media"
                                assetMimetype="[text/html,application/vnd.adobe.contentfragment]"
                                droptarget="contentfragment"
                                resourceType="eaem/components/contentfragment"/>
